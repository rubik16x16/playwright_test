# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = tab
indent_size = 4

[*.{py,js,html}]
trim_trailing_whitespace = true
insert_final_newline = true

[*.{yml,yaml}]
quote_type = double,
indent_style = space
indent_size = 4
